{
  "name": "@grooveshop/recommendations-widget",
  "version": "1.0.0",
  "description": "Plug-and-play recommendation widget for any website",
  "type": "module",
  "main": "dist/widget.umd.js",
  "module": "dist/widget.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.js",
    "format": "prettier --write \"src/**/*.{ts,js,css}\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "recommendations",
    "widget",
    "ecommerce",
    "personalization",
    "machine-learning"
  ],
  "author": "GrooveShop",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {}
}
